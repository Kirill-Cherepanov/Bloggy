{
  "name": "mern-blog-api",
  "version": "1.0.0",
  "description": "MERN blog api",
  "main": "app.js",
  "engines": {
    "node": ">=17.8.0"
  },
  "scripts": {
    "watch-ts": "yarn tsc -w",
    "nodemon-server": "yarn cross-env NODE_PATH=./dist NODE_ENV=development yarn nodemon dist/app",
    "development": "yarn concurrently \"yarn watch-ts\" \"yarn nodemon-server\"",
    "start": "yarn development",
    "build": "yarn tsc",
    "production": "yarn cross-env NODE_PATH=./dist NODE_ENV=production node dist/app",
    "docker:build": "docker build -t bloggy_api .",
    "docker:run": "docker run -p 8080:8080 --name bloggy_api_c bloggy_api"
  },
  "author": "KissMyUSSR",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.5",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.8",
    "typescript": "^4.7.4",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20"
  }
}
